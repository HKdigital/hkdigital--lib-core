	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@playwright/test": "^1.49.0",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/package": "^2.3.7",
		"@sveltejs/vite-plugin-svelte": "^4.0.2",
		"@types/eslint": "^9.6.1",
		"autoprefixer": "^10.4.20",
