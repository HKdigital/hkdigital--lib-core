		},
		"./util/svelte/state-context": {
			"types": "./dist/util/svelte/state-context/index.d.ts",
			"svelte": "./dist/util/svelte/state-context/index.js"
		},
		"./valibot": {
			"types": "./dist/valibot/index.d.ts",
			"svelte": "./dist/valibot/index.js"
		}
