{
	"name": "@hkdigital/lib-sveltekit",
	"version": "0.0.94",
	"author": {
		"name": "HKdigital",
		"url": "https://hkdigital.nl"
	},
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/HKdigital/hkdigital--lib-sveltekit.git"
	},
	"keywords": [
		"component-library",
		"sveltekit",
		"skeleton",
		"tailwindcss",
		"runed",
		"valibot"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "cross-env vite build && npm run package",
		"preview": "vite preview",
		"package": "cross-env svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "cross-env npm run package",
		"publish:npm": "cross-env npm version patch && npm publish --access public && git push",
		"check": "cross-env svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "cross-env svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "cross-env prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "cross-env npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"upgrade:all": "cross-env ncu -u && pnpm install"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*",
		"!dist/**/testdata.*",
		"!dist/**/*.testdata.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./*": "./dist/*"
	},
	"peerDependencies": {
		"@sveltejs/kit": "^2.15.2",
		"svelte": "^5.0.0",
		"runed": "^0.23.0",
		"valibot": "^0.42.1"
	},
	"devDependencies": {
		"@playwright/test": "^1.49.1",
		"@skeletonlabs/skeleton": "3.0.0-next.2",
		"@skeletonlabs/skeleton-svelte": "1.0.0-next.4",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/package": "^2.3.7",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@types/eslint": "^9.6.1",
		"autoprefixer": "^10.4.20",
		"cross-env": "^7.0.3",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"jsdom": "^26.0.0",
		"postcss": "^8.5.1",
		"postcss-mixins": "^11.0.3",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.9",
		"publint": "^0.3.2",
		"standardized-audio-context-mock": "^9.7.15",
		"svelte": "^5.17.5",
		"svelte-check": "^4.1.4",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.7.3",
		"vite": "^6.0.7",
		"vite-imagetools": "^7.0.5",
		"vitest": "^2.1.8"
	},
	"dependencies": {
		"zod": "^3.24.1"
	}
}
