{
	"name": "@hkdigital/lib-sveltekit",
	"version": "0.2.7",
	"author": {
		"name": "HKdigital",
		"url": "https://hkdigital.nl"
	},
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/HKdigital/hkdigital--lib-sveltekit.git"
	},
	"keywords": [
		"component-library",
		"sveltekit",
		"skeleton",
		"tailwindcss",
		"runed",
		"valibot"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"publish:npm": "npm version patch && npm publish --access public && git push",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"upgrade:all": "ncu -u && pnpm install"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*",
		"!dist/**/testdata.*",
		"!dist/**/*.testdata.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./*": "./dist/*",
		"./typedef": "./dist/typedef/index.js"
	},
	"peerDependencies": {
		"@eslint/js": "^9.28.0",
		"@steeze-ui/heroicons": "^2.4.2",
		"@sveltejs/kit": "^2.21.2",
		"eslint-plugin-import": "^2.31.0",
		"pino": "^9.7.0",
		"pino-pretty": "^13.0.0",
		"runed": "^0.23.1",
		"svelte": "^5.33.16",
		"svelte-preprocess": "^6.0.3",
		"valibot": "^1.1.0",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@eslint/js": "^9.28.0",
		"@playwright/test": "^1.52.0",
		"@skeletonlabs/skeleton": "3.0.0-next.2",
		"@skeletonlabs/skeleton-svelte": "1.0.0-next.4",
		"@steeze-ui/heroicons": "^2.4.2",
		"@sveltejs/adapter-auto": "^6.0.1",
		"@sveltejs/package": "^2.3.11",
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"@tailwindcss/typography": "^0.5.16",
		"@testing-library/svelte": "^5.2.7",
		"@testing-library/user-event": "^14.6.1",
		"@types/eslint": "^9.6.1",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.28.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-svelte": "^3.9.1",
		"fake-indexeddb": "^6.0.0",
		"globals": "^16.2.0",
		"jsdom": "^26.1.0",
		"pino": "^9.7.0",
		"pino-pretty": "^13.0.0",
		"postcss": "^8.5.4",
		"postcss-mixins": "^11.0.3",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.12",
		"publint": "^0.3.12",
		"standardized-audio-context-mock": "^9.7.22",
		"svelte": "^5.33.16",
		"svelte-check": "^4.2.1",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.8.3",
		"vite": "^6.3.5",
		"vite-imagetools": "^7.1.0",
		"vitest": "^3.2.2",
		"zod": "^3.24.2"
	}
}
